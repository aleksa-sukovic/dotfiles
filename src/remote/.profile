# === Variables ===
export EDITOR=/usr/bin/vim

#  === Aliases ===
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

alias sudo="sudo "

alias l="ls -l"
alias la="ls -la"
alias lsd='ls -l | grep "^d"'
alias rm='rm -i'

alias fs="du -sh"

alias dcont="docker container"
alias dcomp="docker compose"
